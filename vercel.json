{
  "buildCommand": "npm run build",
  "outputDirectory": "public",
  "devCommand": "npm start",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/proxy", "destination": "/api/proxy" },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
