{
  "buildCommand": "npm run build",
  "outputDirectory": "public",
  "devCommand": "npm start",
  "installCommand": "npm install",
  "framework": null,
  "functions": {
    "api/**/*.{js,ts}": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
